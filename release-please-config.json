{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    ".": {
      "release-type": "node"
    }
  },
  "changelog-host": "https://github.mpi-internal.com",
  "include-v-in-tag": false,
  "pull-request-title-pattern": "[skip ci] chore${scope}: release${component} ${version}",
  "group-pull-request-title-pattern": "[skip ci] chore${scope}: release${component} ${version}",
  "extra-label": "Ready to Merge",
  "changelog-sections": [
    { "type": "feat", "section": "Features" },
    { "type": "feature", "section": "Features" },
    { "type": "fix", "section": "Bug Fixes" },
    { "type": "perf", "section": "Performance Improvements" },
    { "type": "revert", "section": "Reverts" },
    { "type": "docs", "section": "Documentation" },
    { "type": "style", "section": "Styles" },
    { "type": "chore", "section": "Miscellaneous Chores" },
    { "type": "refactor", "section": "Code Refactoring" },
    { "type": "test", "section": "Tests" },
    { "type": "build", "section": "Build System" },
    { "type": "ci", "section": "Continuous Integration" }
  ]
}

